{
  "name": "jianshu",
  "version": "0.1.5",
  "author": {
    "name": "jiayi",
    "url": "https://github.com/jiayisheji"
  },
  "bugs": {
    "url": "https://github.com/jiayisheji/jianshu/issues"
  },
  "description": "这是一个模仿简书从前端到后台的项目",
  "keywords": [
    "typescript",
    "nest",
    "npm",
    "ts",
    "tsc",
    "node",
    "mongodb",
    "express"
  ],
  "license": "MIT",
  "main": "src/bootstrap.ts",
  "scripts": {
    "start": "npm run serve:dev",
    "build": "npm run lint && npm run clean:dist && npm run transpile",
    "build:dist": "npm run clean:dist && npm run transpile",
    "clean": "npm run clean:dist",
    "clean:all": "npm run clean:coverage && npm run clean:dist",
    "clean:coverage": "rimraf './coverage'",
    "clean:dist": "rimraf './dist'",
    "serve:dev": "nodemon -e ts --watch src --watch .env",
    "serve:dist": "node dist/bootstrap.js",
    "test": "cross-env NODE_ENV=test jest --runInBand  --forceExit",
    "test:verbose": "npm run test -- --verbose",
    "test:coverage": "npm run clean:coverage && npm run test -- --coverage",
    "report-codeclimate": "codeclimate-test-reporter < coverage/lcov.info",
    "lint": "tslint --project tsconfig.json -c ./tslint.json 'src/**/*.ts'  --exclude 'src/**/*.d.ts' --exclude 'src/**/*.spec.ts'  --exclude 'src/**/*.spec-data.ts'",
    "release": "standard-version",
    "security": "nsp check",
    "transpile": "tsc"
  },
  "dependencies": {
    "@nestjs/common": "^4.5.2",
    "@nestjs/core": "^4.5.2",
    "@nestjs/microservices": "^4.5.2",
    "@nestjs/swagger": "^1.1.3",
    "@nestjs/testing": "^4.5.2",
    "@nestjs/websockets": "^4.5.2",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-rate-limit": "^2.11.0",
    "express-validator": "^4.3.0",
    "helmet": "^3.9.0",
    "ioredis": "^3.2.2",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.13.7",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "rate-limit-redis": "^1.3.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "@types/compression": "0.0.35",
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.0",
    "@types/helmet": "0.0.37",
    "@types/ioredis": "^3.2.4",
    "@types/lodash": "^4.14.91",
    "@types/mongodb": "^2.2.18",
    "@types/mongoose": "^4.7.30",
    "@types/node": "^8.0.28",
    "@types/redis": "^2.8.3",
    "mockgoose": "^7.3.3",
    "nodemon": "^1.14.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.5.2"
  }
}